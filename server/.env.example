# StreamFinity Server Configuration
NODE_ENV=production

# Server Ports
SF_HTTP_PORT=3010
SF_WS_PORT=8090

# Database
DATABASE_URL=postgresql://streamfinity:streamfinity@127.0.0.1:5432/streamfinity
REDIS_URL=redis://127.0.0.1:6379

# CDN / Public URL
CDN_URL=https://tik.starline-agency.xyz

# Upstream Platform URLs (fallback only)
TIKFINITY_HOST=https://tikfinity.zerody.one
TIKFINITY_FALLBACK=http://tikfinity-origin.zerody.one
STREAMTORY_HOST=https://tiktory.app

# Security (comma-separated API keys, leave empty to disable)
SF_API_KEYS=

# Logging
LOG_LEVEL=info
